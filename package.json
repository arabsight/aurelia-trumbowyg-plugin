{
  "name": "aurelia-trumbowyg-plugin",
  "version": "0.1.6",
  "description": "A trumbowyg wrapper for Aurelia.",
  "scripts": {
    "prepush": "gulp prepare-release --bump=patch",
    "push":
      "git add --all && git commit -m \"$(date)\" && git push origin master && npm publish",
    "test": "gulp test"
  },
  "keywords": ["aurelia", "plugin", "trumbowyg"],
  "homepage": "https://github.com/arabsight/aurelia-trumbowyg-plugin",
  "bugs": {
    "url": "https://github.com/arabsight/aurelia-trumbowyg-plugin/issues"
  },
  "license": "MIT",
  "author": "Rabah Ghodbane <r-ghodbane@live.com>",
  "main": "dist/commonjs/index.js",
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "https://github.com/arabsight/aurelia-trumbowyg-plugin"
  },
  "devDependencies": {
    "aurelia-tools": "^2.0.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "conventional-changelog": "1.1.24",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-bump": "^3.1.1",
    "gulp-eslint": "^4.0.2",
    "gulp-yuidoc": "^0.1.2",
    "isparta": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-jspm": "2.2.3",
    "require-dir": "^1.0.0",
    "run-sequence": "^2.2.1",
    "vinyl-paths": "^2.1.0",
    "yargs": "^11.0.0"
  },
  "jspm": {
    "registry": "npm",
    "main": "index",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "devDependencies": {
      "aurelia-polyfills": "^1.2.2"
    }
  },
  "aurelia": {
    "build": {
      "resources": ["trumbowyg-editor"]
    }
  }
}
